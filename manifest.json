{
  "manifest_version": 2,

  "name": "Requests",
  "description": "Requests",
  "version": "1.0",
  "devtools_page": "devtools.html",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Live HTTP headers"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "jq.js",
        "content_script.js"
      ]
    }
  ]
}